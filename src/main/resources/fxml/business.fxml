<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml/1" AnchorPane.bottomAnchor="0.0"
  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0"
  AnchorPane.topAnchor="0.0" alignment="TOP_CENTER" cache="true"
  vgap="5.0" xmlns="http://javafx.com/javafx/8.0.121">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" minWidth="10.0"/>
    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0"/>
    <ColumnConstraints/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" percentHeight="10.0" valignment="CENTER" vgrow="SOMETIMES"/>
    <RowConstraints maxHeight="50.0" minHeight="10.0" percentHeight="7.0" valignment="CENTER"
      vgrow="NEVER"/>
    <RowConstraints maxHeight="50.0" minHeight="10.0" percentHeight="7.0" valignment="CENTER"
      vgrow="NEVER"/>
    <RowConstraints maxHeight="100.0" minHeight="10.0" percentHeight="10.0" valignment="CENTER"
      vgrow="NEVER"/>
    <RowConstraints maxHeight="-Infinity" minHeight="10.0" percentHeight="20.0" vgrow="SOMETIMES"/>
    <RowConstraints maxHeight="400.0" minHeight="10.0" percentHeight="51.0" vgrow="SOMETIMES"/>

  </rowConstraints>
  <children>
    <Text GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER"
      GridPane.rowIndex="0" boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE"
      strokeWidth="0.0"
      textAlignment="CENTER" textOrigin="CENTER" fx:id="businessName">
      <font>
        <Font size="45.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="1"
      GridPane.valignment="CENTER"
      GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Rating:">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <Text GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1"
      GridPane.valignment="CENTER"
      GridPane.vgrow="NEVER" boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE"
      strokeWidth="0.0" fx:id="rating">
      <font>
        <Font name="System" size="20.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="2"
      GridPane.valignment="CENTER"
      GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Location:">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <Text GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="2"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" boundsType="LOGICAL_VERTICAL_CENTER"
      strokeType="OUTSIDE"
      strokeWidth="0.0" fx:id="address">
      <font>
        <Font name="System" size="20.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="3"
      GridPane.valignment="CENTER"
      GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Hours: ">
      <font>
        <Font name="System Bold" size="20.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <HBox GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.valignment="CENTER"
      prefHeight="100.0"
      prefWidth="200.0">
      <children>
        <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0"
          textOrigin="CENTER" wrappingWidth="250.0" fx:id="hours">
          <font>
            <Font size="16.0"/>
          </font>
        </Text>
        <Text boundsType="LOGICAL_VERTICAL_CENTER" layoutX="10.0" layoutY="28.0"
          strokeType="OUTSIDE" strokeWidth="0.0" textOrigin="CENTER" wrappingWidth="250.0"
          fx:id="weekendHours">
          <font>
            <Font size="16.0"/>
          </font>
        </Text>
      </children>
    </HBox>
    <GridPane GridPane.columnSpan="2" GridPane.hgrow="ALWAYS" GridPane.rowIndex="4"
      GridPane.vgrow="ALWAYS" prefWidth="602.0">
      <columnConstraints>
        <ColumnConstraints halignment="LEFT" hgrow="NEVER" minWidth="10.0"/>
        <ColumnConstraints hgrow="SOMETIMES" maxWidth="541.0" minWidth="10.0" prefWidth="541.0"/>
      </columnConstraints>
      <rowConstraints>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="50.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="35.0" vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" prefHeight="125.0"
          vgrow="SOMETIMES"/>
        <RowConstraints maxHeight="-Infinity" minHeight="10.0" vgrow="SOMETIMES"/>
      </rowConstraints>
      <children>
        <Separator GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS"
          GridPane.valignment="TOP" GridPane.vgrow="NEVER" prefWidth="200.0"/>
        <Label GridPane.columnSpan="2" GridPane.halignment="LEFT" GridPane.valignment="BOTTOM"
          text="Write A Review:">
          <font>
            <Font name="System Bold" size="20.0"/>
          </font>
          <GridPane.margin>
            <Insets bottom="10.0"/>
          </GridPane.margin>
        </Label>
        <Label GridPane.halignment="RIGHT" GridPane.rowIndex="1" GridPane.valignment="TOP"
          alignment="TOP_LEFT"
          text="Rating:">
          <font>
            <Font size="18.0"/>
          </font>
          <GridPane.margin>
            <Insets right="5.0"/>
          </GridPane.margin>
        </Label>
        <TextField GridPane.columnIndex="1" GridPane.rowIndex="1" GridPane.valignment="TOP"
          maxWidth="-Infinity"
          prefHeight="25.0" prefWidth="75.0" promptText="1-5" fx:id="ratingTextField">
          <GridPane.margin>
            <Insets top="3.0"/>
          </GridPane.margin>
        </TextField>
        <Label GridPane.halignment="RIGHT" GridPane.rowIndex="2" GridPane.valignment="TOP"
          alignment="TOP_LEFT"
          contentDisplay="CENTER" text="Text:">
          <font>
            <Font size="18.0"/>
          </font>
          <GridPane.margin>
            <Insets right="5.0"/>
          </GridPane.margin>
        </Label>
        <TextArea GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS"
          GridPane.rowIndex="2" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS"
          prefHeight="100.0" prefWidth="541.0" fx:id="reviewTextArea"/>
        <Button GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="3"
          GridPane.valignment="TOP" mnemonicParsing="false" text="Submit"
          fx:id="submitReviewButton">
          <GridPane.margin>
            <Insets top="5.0"/>
          </GridPane.margin>
          <font>
            <Font name="System Bold" size="14.0"/>
          </font>
          <cursor>
            <Cursor fx:constant="HAND"/>
          </cursor>
        </Button>
      </children>
    </GridPane>
    <VBox GridPane.columnSpan="2" GridPane.halignment="LEFT" GridPane.hgrow="ALWAYS"
      GridPane.rowIndex="5" GridPane.valignment="TOP" GridPane.vgrow="ALWAYS">
      <children>
        <Label VBox.vgrow="NEVER" graphicTextGap="0.0" text="All Reviews &amp; Tips:"
          textAlignment="CENTER" fx:id="reviewLabel">
          <font>
            <Font name="System Bold" size="20.0"/>
          </font>
          <VBox.margin>
            <Insets bottom="10.0"/>
          </VBox.margin>
        </Label>
        <TableView fx:id="reviewsTable"/>
      </children>
    </VBox>
  </children>
  <padding>
    <Insets bottom="5.0" left="10.0" right="10.0" top="5.0"/>
  </padding>
</GridPane>
