<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<GridPane xmlns:fx="http://javafx.com/fxml" AnchorPane.bottomAnchor="0.0"
  AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"
  alignment="CENTER" cache="true" xmlns="http://javafx.com/javafx">
  <columnConstraints>
    <ColumnConstraints hgrow="NEVER" minWidth="10.0"/>
    <ColumnConstraints halignment="LEFT" hgrow="SOMETIMES" minWidth="10.0"/>
    <ColumnConstraints/>
  </columnConstraints>
  <rowConstraints>
    <RowConstraints minHeight="10.0" percentHeight="13.0" valignment="CENTER" vgrow="SOMETIMES"/>
    <RowConstraints maxHeight="50.0" minHeight="10.0" percentHeight="7.0" vgrow="NEVER"/>
    <RowConstraints maxHeight="70.0" minHeight="10.0" percentHeight="10.0" valignment="CENTER"
      vgrow="NEVER"/>
    <RowConstraints maxHeight="100.0" minHeight="10.0" percentHeight="15.0" valignment="CENTER"
      vgrow="NEVER"/>
    <RowConstraints minHeight="10.0" percentHeight="55.0" vgrow="SOMETIMES"/>
  </rowConstraints>
  <children>
    <Text GridPane.columnIndex="0" GridPane.columnSpan="2" GridPane.halignment="CENTER"
      GridPane.rowIndex="0" boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE"
      strokeWidth="0.0" textAlignment="CENTER" textOrigin="CENTER" fx:id="businessName">
      <font>
        <Font size="65.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="1"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Rating:">
      <font>
        <Font name="System Bold" size="22.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <Text GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="1"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" boundsType="LOGICAL_VERTICAL_CENTER"
      strokeType="OUTSIDE" strokeWidth="0.0" fx:id="rating">
      <font>
        <Font size="22.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="2"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Location:">
      <font>
        <Font name="System Bold" size="22.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <Text GridPane.columnIndex="1" GridPane.halignment="LEFT" GridPane.rowIndex="2"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" boundsType="LOGICAL_VERTICAL_CENTER"
      strokeType="OUTSIDE" strokeWidth="0.0" fx:id="address">
      <font>
        <Font size="22.0"/>
      </font>
    </Text>
    <Label GridPane.halignment="RIGHT" GridPane.hgrow="NEVER" GridPane.rowIndex="3"
      GridPane.valignment="CENTER" GridPane.vgrow="NEVER" alignment="TOP_LEFT" text="Hours: ">
      <font>
        <Font name="System Bold" size="22.0"/>
      </font>
      <GridPane.margin>
        <Insets right="15.0"/>
      </GridPane.margin>
    </Label>
    <HBox GridPane.columnIndex="1" GridPane.rowIndex="3" GridPane.valignment="CENTER"
      prefHeight="100.0" prefWidth="200.0">
      <children>
        <Text boundsType="LOGICAL_VERTICAL_CENTER" strokeType="OUTSIDE" strokeWidth="0.0"
          textOrigin="CENTER" wrappingWidth="250.0" fx:id="hours">
          <font>
            <Font size="16.0"/>
          </font>
        </Text>
        <Text boundsType="LOGICAL_VERTICAL_CENTER" layoutX="10.0" layoutY="28.0"
          strokeType="OUTSIDE" strokeWidth="0.0" textOrigin="CENTER" wrappingWidth="250.0"
          fx:id="weekendHours">
          <font>
            <Font size="16.0"/>
          </font>
        </Text>
      </children>
    </HBox>
    <VBox GridPane.columnSpan="2" GridPane.halignment="CENTER" GridPane.hgrow="NEVER"
      GridPane.rowIndex="4" alignment="CENTER">
      <children>
        <Label VBox.vgrow="NEVER" alignment="CENTER" contentDisplay="CENTER" graphicTextGap="0.0"
          text="Reviews &amp; Tips:" textAlignment="CENTER" fx:id="reviewLabel">
          <font>
            <Font name="System Bold" size="22.0"/>
          </font>
        </Label>
        <TableView fx:id="reviewsTable"/>
      </children>
    </VBox>
  </children>
  <padding>
    <Insets bottom="3.0" left="10.0" right="10.0" top="5.0"/>
  </padding>
</GridPane>
